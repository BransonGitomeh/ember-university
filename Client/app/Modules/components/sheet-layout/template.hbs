<table class="stripped">
  <thead>
    <th>firstname</th>
    <th>middlename</th>
    <th>lastname</th>

    <th>gender</th>
    <th>dob</th>
    <th>nationality</th>

    <th>idNo</th>
    <th>residentialarea</th>
    <th>phonenumber</th>
  </thead>
<tbody>
  {{#each students as |user|}}
    <tr>
      <td>{{user.firstname}}</td>
      <td>{{user.middlename}}</td>
      <td>{{user.lastname}}</td>

      <td>{{user.gender}}</td>
      <td>{{user.dob}}</td>
      <td>{{user.nationality}}</td>

      <td>{{user.idNo}}</td>
      <td>{{user.residentialarea}}</td>
      <td>{{user.phonenumber}}</td>
    </tr>
  {{/each}}
</tbody>
</table>
